<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.quickjob">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".VendorSettings"
            android:label="SETTINGS"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="com.android.vendorsettings" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".VendorPreviousOrders"
            android:label="PREVIOUS ORDERS"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="com.android.vendorpreviousorder" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".VendorEditDetails"
            android:label="EDIT DETAILS"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="com.android.vendoreditdetails" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".VendorLogin"
            android:label="VENDOR LOGIN">
            <intent-filter>
                <action android:name="com.android.vendorlogin" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DeveloperOptions"
            android:label="DEVELOPER OPTIONS"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="com.android.developeroptions" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".VendorAddition"
            android:label="VENDOR ADDITION">
            <intent-filter>
                <action android:name="com.android.vendoraddition" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".VendorList"
            android:label="VENDOR LIST"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="com.android.vendorlist" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".VendorProfile"
            android:label="Vendor Profile"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="com.android.vendorprofile" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PreviousOrders"
            android:label="PREVIOUS ORDERS"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="com.android.perviousorder" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Settings"
            android:label="SETTINGS"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="com.android.settings" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EditDetails"
            android:label="EDIT DETAILS"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="com.android.editdetails" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".UserProfile"
            android:label="USER PROFILE"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.SEND" />
                <action android:name="com.android.userprofile" />

                <data android:mimeType="*/*" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Login"
            android:label="LOGIN">
            <intent-filter>
                <action android:name="com.android.login" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="SIGN UP">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>